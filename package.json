{
  "name": "bookstore",
  "version": "1.0.0",
  "description": "API used to sell books",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "sbom:syft": "docker run --rm -v \"$(pwd)\":/project anchore/syft:latest dir:/project -o cyclonedx-json > sbom.cdx.json",
    "prepare": "husky install",
    "dev": "node --inspect=0.0.0.0 --require ./src/infrastructure/suppress-experimental-fetch-warnings.cjs --watch --trace-warnings --experimental-loader ts-node/esm ./init.ts",
    "start": "node --require ./src/infrastructure/suppress-experimental-fetch-warnings.cjs --trace-warnings --experimental-loader ts-node/esm ./init.ts",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "DATABASE_URL=$DATABASE_URL_TEST jest -c src/e2e/jest.e2e.config.ts --runInBand",
    "lint": "eslint . --ext .ts",
    "seed": "ts-node src/infrastructure/database/seed.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leonardovff/nodejs-bookstore-api.git"
  },
  "keywords": [
    "node.js",
    "typescript",
    "prisma",
    "mongodb",
    "javascript",
    "api",
    "restapi"
  ],
  "author": "leonardovff",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/leonardovff/nodejs-bookstore-api/issues"
  },
  "homepage": "https://github.com/leonardovff/nodejs-bookstore-api#readme",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.3",
    "jest": "^29.4.2",
    "prettier": "^2.8.4",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@prisma/client": "^4.16.2",
    "express": "^4.18.2",
    "express-oas-generator": "^1.0.46",
    "joi": "^17.7.1"
  }
}
